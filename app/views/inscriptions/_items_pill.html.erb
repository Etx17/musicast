<% if inscription.has_complete_requirement_items? %>
<div class="rounded-pill  border-success text-success bg-primary-light py-1 px-2">
  <i class="fas fa-check text-success"></i> <%= t('inscriptions.pills.items.complete') %>
</div>
<% else %>
  <div class="rounded-pill text-secondary bg-secondary-secondary py-1 px-2">
    <i class="fas fa-warning text-info"></i> <%= t('inscriptions.pills.items.incomplete') %>
  </div>
<% end %>
